{
  "name": "limbo",
  "version": "0.5.5",
  "description": "Factory/Loader of mongoose",
  "main": "./lib/limbo.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require coffee-script/register --require should --reporter spec test/main.coffee",
    "prepublish": "rm -rf lib && coffee -o lib -c src"
  },
  "engines": {
    "node": ">= 4"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "database",
    "rpc",
    "loader"
  ],
  "license": "MIT",
  "author": "teambition",
  "repository": {
    "type": "git",
    "url": "git@github.com:teambition/limbo.git"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "mocha": "^2.4.5",
    "mongoose": "4.7.9",
    "should": "^8.2.2"
  },
  "dependencies": {
    "axon-rpc": "github:orangemi/axon-rpc",
    "axon-tls": "^2.2.1"
  }
}
